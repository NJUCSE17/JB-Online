{
    "name": "doowzs/Class-Forum",
    "description": "Class forum project (JB Online).",
    "keywords": ["framework", "laravel", "forum"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.8",
        "ext-json": "^1.6",
        "appstract/laravel-blade-directives": "^1.3.0",
        "arcanedev/no-captcha": "^7.0",
        "barryvdh/elfinder-flysystem-driver": "^0.2.1",
        "barryvdh/laravel-elfinder": "^0.4.1",
        "beyondcode/laravel-self-diagnosis": "^1.0.2",
        "creativeorange/gravatar": "~1.0",
        "cybercog/laravel-love": "^5.2",
        "darkaonline/l5-swagger": "5.7.*",
        "davejamesmiller/laravel-breadcrumbs": "^5.2",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "graham-campbell/flysystem": "^5.1",
        "hieu-le/active": "^3.5.1",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^4.0.1",
        "laravel/telescope": "^1.0",
        "laravel/tinker": "^1.0.8",
        "league/glide": "^1.4.0",
        "league/glide-laravel": "^1.0",
        "mews/purifier": "^2.1.2",
        "nesbot/carbon": "2.9.0 as 1.36.0",
        "spatie/laravel-backup": "^6.0",
        "spatie/laravel-html": "^2.19.8",
        "spatie/laravel-permission": "^2.29",
        "torann/geoip": "^1.0.8",
        "webpatser/laravel-uuid": "^3.0.1",
        "willvincent/feeds": "^1.1.7",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2.1",
        "barryvdh/laravel-ide-helper": "^2.5.3",
        "codedungeon/phpunit-result-printer": "^0.23.4",
        "filp/whoops": "^2.3.1",
        "friendsofphp/php-cs-fixer": "^2.13.1",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^2.1.1",
        "phpunit/phpunit": "^7.5.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan package:discover"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "phpunit": [
            "phpunit > output.txt -d memory_limit=2048M"
        ],
        "coverage-xml": [
            "phpunit --coverage-clover ./coverage.xml"
        ],
        "coverage-html": [
            "phpunit --coverage-html ./coverage"
        ],
        "coverage-text": [
            "phpunit --coverage-text"
        ],
        "format": [
            "./vendor/bin/php-cs-fixer fix app/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix config/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix database/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix resources/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix routes/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix tests/  --show-progress=estimating"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
